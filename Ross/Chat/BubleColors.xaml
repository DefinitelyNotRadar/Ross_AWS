<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Ross">

    <Color x:Key="WordDarkBlue">#0c6991</Color>
    <SolidColorBrush x:Key="WordDarkBlueBrush" Color="{StaticResource WordDarkBlue}" />

    <Color x:Key="WordTransparentBlue">#003099c5</Color>
    <SolidColorBrush x:Key="WordTransparentBlueBrush" Color="{StaticResource WordTransparentBlue}" />

    <Color x:Key="WordLightBlue">#45b6e5</Color>
    <SolidColorBrush x:Key="WordLightBlueBrush" Color="{StaticResource WordLightBlue}" />


    <Color x:Key="WordDarkGray">#283747</Color>
    <SolidColorBrush x:Key="WordDarkGrayBrush" Color="{StaticResource WordDarkGray}" />

    <Color x:Key="WordVeryDarkGray">#212f3d</Color>
    <SolidColorBrush x:Key="WordVeryDarkGrayBrush" Color="{StaticResource WordVeryDarkGray}" />

    <Color x:Key="WordVeryVeryDarkGray">#1c2833</Color>
    <SolidColorBrush x:Key="WordVeryVeryDarkGrayBrush" Color="{StaticResource WordVeryVeryDarkGray}" />
    
</ResourceDictionary>